<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
   prefHeight="577.0" prefWidth="740.0" xmlns="http://javafx.com/javafx/20.0.1"
   xmlns:fx="http://javafx.com/fxml/1" fx:controller="SignInController">
   <children>
      <AnchorPane prefHeight="561.0" prefWidth="649.0">
         <children>
            <AnchorPane fx:id="right_scene" layoutY="2.0" prefHeight="578.0" prefWidth="374.0"
               styleClass="left-form" stylesheets="@z-SignInDesign.css" />
            <AnchorPane layoutX="374.0" layoutY="6.0" prefHeight="577.0" prefWidth="364.0"
               styleClass="right-form" stylesheets="@z-SignInDesign.css">
               <children>
                  <Label layoutX="68.0" layoutY="160.0" text="Sign In">
                     <font>
                        <Font name="Book Antiqua" size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="email" layoutX="73.0" layoutY="254.0" onAction="#login"
                     prefHeight="30.0" prefWidth="235.0" styleClass="textfield"
                     stylesheets="@z-SignInDesign.css" />
                  <PasswordField fx:id="password" layoutX="73.0" layoutY="326.0" onAction="#login"
                     prefHeight="30.0" prefWidth="235.0" styleClass="passwordfield"
                     stylesheets="@z-SignInDesign.css" />
                  <Label layoutX="67.0" layoutY="183.0"
                     text="Enter your email and password to sign in.">
                     <font>
                        <Font size="10.0" />
                     </font>
                  </Label>
                  <Label layoutX="70.0" layoutY="237.0" text="Email Address">
                     <font>
                        <Font name="Book Antiqua" size="10.0" />
                     </font>
                  </Label>
                  <Button fx:id="loginBtn" layoutX="62.0" layoutY="400.0" mnemonicParsing="false"
                     onAction="#login" prefHeight="43.0" prefWidth="263.0" styleClass="sign-in"
                     stylesheets="@z-SignInDesign.css" text="Sign In" textFill="WHITE">
                     <font>
                        <Font name="Bookman Old Style" size="12.0" />
                     </font>
                  </Button>
                  <ImageView fitHeight="134.0" fitWidth="182.0" layoutX="92.0" layoutY="69.0"
                     pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/taylors-logo-latest.png" />
                     </image>
                  </ImageView>
                  <Button fx:id="close" layoutX="342.0" layoutY="1.0" mnemonicParsing="false"
                     onAction="#close" prefHeight="21.0" prefWidth="0.0" styleClass="close"
                     stylesheets="@z-SignInDesign.css" text="X">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
                  <Label layoutX="70.0" layoutY="310.0" text="Password">
                     <font>
                        <Font name="Book Antiqua" size="10.0" />
                     </font>
                  </Label>
                  <Button fx:id="signUp" layoutX="169.0" layoutY="444.0" mnemonicParsing="false"
                     onAction="#signUp" styleClass="signUp" stylesheets="@z-SignInDesign.css"
                     text="Sign Up">
                     <font>
                        <Font size="10.0" />
                     </font>
                  </Button>
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</StackPane>